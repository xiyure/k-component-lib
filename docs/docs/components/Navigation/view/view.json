{
  "name": "View",
  "props": [
    {
      "displayName": "data",
      "type": "ViewData[]",
      "description": "绑定的数据",
      "defaultValue": "",
      "tip": ""
    },
    {
      "displayName": "defaultActive",
      "type": "string | number",
      "description": "默认激活视图项",
      "defaultValue": "",
      "tip": ""
    },
    {
      "displayName": "draggable",
      "type": "boolean",
      "description": "是否可拖拽",
      "defaultValue": "false",
      "tip": ""
    },
    {
      "displayName": "collapse",
      "type": "boolean",
      "description": "是否折叠",
      "defaultValue": "false",
      "tip": ""
    },
    {
      "displayName": "show-custom-control",
      "type": "boolean",
      "description": "是否显示自定义控制按钮",
      "defaultValue": "false",
      "tip": ""
    },
    {
      "displayName": "use-tree",
      "type": "boolean",
      "description": "是否使用树形结构",
      "defaultValue": "",
      "tip": ""
    },
    {
      "displayName": "treeConfig",
      "type": "object",
      "description": "树形结构配置",
      "defaultValue": "",
      "tip": "TreeViewConfig"
    },
    {
      "displayName": "showCount",
      "type": "boolean",
      "description": "是否显示视图数据量",
      "defaultValue": "true",
      "tip": ""
    },
    {
      "displayName": "props",
      "type": "object",
      "description": "自定义视图项属性",
      "defaultValue": "",
      "tip": "ViewAttributes"
    },
    {
      "displayName": "simple",
      "type": "boolean",
      "description": "是否使用精简模式",
      "defaultValue": "false",
      "tip": "",
      "version": "1.1.20"
    },
    {
      "displayName": "height",
      "type": "string | number",
      "description": "视图高度",
      "defaultValue": "",
      "tip": "",
      "version": "1.1.20"
    },
    {
      "displayName": "v-model / modelValue",
      "type": "string | number",
      "description": "当前激活视图项",
      "defaultValue": "",
      "tip": "",
      "version": "1.1.21"
    }
  ],
  "subProps": {
    "TreeViewConfig": [
      {
        "displayName": "nodeKey",
        "type": "string | number",
        "description": "节点唯一标识",
        "defaultValue": "value",
        "tip": ""
      },
      {
        "displayName": "emptyText",
        "type": "string",
        "description": "空数据时显示的文字",
        "defaultValue": "",
        "tip": ""
      },
      {
        "displayName": "expand-on-click-node",
        "type": "boolean",
        "description": "是否点击节点展开, 为 false 时只有点箭头才会展开",
        "defaultValue": "",
        "tip": ""
      },
      {
        "displayName": "default-expanded-keys",
        "type": "array",
        "description": "默认展开的节点",
        "defaultValue": "",
        "tip": "Array<string | number>"
      },
      {
        "displayName": "showArrow",
        "type": "boolean",
        "description": "是否显示箭头",
        "defaultValue": "true",
        "tip": ""
      },
      {
        "displayName": "accordion",
        "type": "boolean",
        "description": "是否每次只打开一个同级树节点展开",
        "defaultValue": "false",
        "tip": ""
      },
      {
        "displayName": "indent",
        "type": "number",
        "description": "相邻级节点间的水平缩进，单位为px",
        "defaultValue": "18",
        "tip": ""
      },
      {
        "displayName": "lazy",
        "type": "boolean",
        "description": "是否懒加载子节点",
        "defaultValue": "false",
        "tip": ""
      },
      {
        "displayName": "icon",
        "type": "string | Component | Function",
        "description": "子节点图标",
        "defaultValue": "",
        "tip": "() => string | Component"
      },
      {
        "displayName": "expandIcon",
        "type": "string | Component | Function",
        "description": "父节点展开图标",
        "defaultValue": "",
        "tip": "() => string | Component"
      },
      {
        "displayName": "collapseIcon",
        "type": "string | Component | Function",
        "description": "父节点收起图标",
        "defaultValue": "",
        "tip": "() => string | Component"
      },
      {
        "displayName": "load",
        "type": "Function",
        "description": "加载子树数据的方法，仅当 lazy 属性为true 时生效",
        "defaultValue": "",
        "tip": "(node, resolve, reject) => void"
      }
    ]
  },
  "events": [
    {
      "displayName": "refresh",
      "description": "点击刷新按钮时触发",
      "type": "Function",
      "tip": "() => void"
    },
    {
      "displayName": "change",
      "description": "视图切换时触发",
      "type": "Function",
      "tip": "({ value: string | number, data: ViewData }) => void"
    },
    {
      "displayName": "remove",
      "description": "点击删除视图按钮时触发",
      "type": "Function",
      "tip": "({ value: string | number, data: ViewData }) => void"
    },
    {
      "displayName": "drag",
      "description": "视图拖拽后触发",
      "type": "Function",
      "tip": "({ value: string | number, data: ViewData }) => void"
    },
    {
      "displayName": "visible",
      "description": "视图显示/隐藏时触发",
      "type": "Function",
      "tip": "(isVisible: boolean) => void"
    }
  ],
  "slots": [
    {
      "displayName": "default",
      "description": "默认视图内容",
      "parameters": ""
    },
    {
      "displayName": "[active]",
      "description": "自定义激活视内容， active为视图项的value",
      "parameters": ""
    },
    {
      "displayName": "header",
      "description": "自定义头部内容",
      "parameters": ""
    },
    {
      "displayName": "refresh",
      "description": "自定义刷新按钮",
      "parameters": "object",
      "tip": "{ data: ViewData }"
    },
    {
      "displayName": "label",
      "description": "自定义视图项",
      "parameters": "",
      "tip": ""
    },
    {
      "displayName": "custom-header",
      "description": "自定义视图内容",
      "parameters": ""
    }
  ],
  "methods": [
    {
      "displayName": "expand",
      "description": "展开视图",
      "type": "Function",
      "tip": "() => void"
    },
    {
      "displayName": "collapse",
      "description": "收起视图",
      "type": "Function",
      "tip": "() => void"
    },
    {
      "displayName": "toggle",
      "description": "切换视图展开/收起状态",
      "type": "Function",
      "tip": "() => void"
    },
    {
      "displayName": "isExpand",
      "description": "获取视图是否展开",
      "type": "Function",
      "tip": "() => boolean"
    },
    {
      "displayName": "isCollapse",
      "description": "获取视图是否收起",
      "type": "Function",
      "tip": "() => boolean"
    }
  ]
}