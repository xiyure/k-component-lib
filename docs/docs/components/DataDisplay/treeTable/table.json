      },
      {
        "displayName": "pageSize",
        "description": "每页条数",
        "type": "number"
      },
      {
        "displayName": "pageSizes",
        "description": "每页条数选项",
        "type": "number[]"
      },
      {
        "displayName": "currentPage",
        "description": "当前页数",
        "type": "number"
      },
      {
        "displayName": "pageCount",
        "description": "总页数",
        "type": "number"
      },
      {
        "displayName": "layout",
