{
  "name": "TreeTable",
  "props": [
    {
      "displayName": "data",
      "type": "any[]",
      "description": "表格数据",
      "defaultValue": "",
      "tip": ""
    },
    {
      "displayName": "column",
      "type": "object",
      "description": "表格列配置",
      "defaultValue": "",
      "tip": "columnConfigType[]"
    },
    {
      "displayName": "size",
      "type": "enum",
      "description": "表格大小",
      "defaultValue": "small",
      "tip": "'medium' | 'small' | 'mini'"
    },
    {
      "displayName": "height",
      "type": "number | string",
      "description": "表格高度",
      "defaultValue": "100%",
      "tip": ""
    },
    {
      "displayName": "useTree",
      "type": "boolean",
      "description": "是否使用树形表格",
      "defaultValue": "false",
      "tip": ""
    },
    {
      "displayName": "isRemoteQuery",
      "type": "boolean",
      "description": "是否远程搜索表格数据",
      "defaultValue": "false",
      "tip": ""
    },
    {
      "displayName": "isServerPaging",
      "type": "boolean",
      "description": "是否服务端分页",
      "defaultValue": "false",
      "tip": ""
    },
    {
      "displayName": "showDescription",
      "type": "boolean",
      "description": "表格数据描述，包含筛选条件、统计等内容",
      "defaultValue": "true",
      "tip": ""
    },
    {
      "displayName": "showHeaderTools",
      "type": "boolean",
      "description": "是否显示表头工具栏",
      "defaultValue": "true",
      "tip": ""
    },
    {
      "displayName": "showBatchOperation",
      "type": "boolean",
      "description": "是否显示批量操作（只在type=checkbox时有效）",
      "defaultValue": "false",
      "tip": ""
    },
    {
      "displayName": "showColumnMenu",
      "type": "boolean",
      "description": "是否显示列头菜单",
      "defaultValue": "false",
      "tip": ""
    },
    {
      "displayName": "showDragColumn",
      "type": "boolean",
      "description": "是否显示拖拽列",
      "defaultValue": "false",
      "tip": ""
    },
    {
      "displayName": "batchOperations",
      "type": "object",
      "description": "批量操作配置",
      "defaultValue": "",
      "tip": "{label: string, handler: () => any}[]"
    },
    {
      "displayName": "cellClickToggleHighlight",
      "type": "boolean",
      "description": "单元格单击是否高亮/取消高亮状态切换",
      "defaultValue": "true",
      "tip": ""
    },
    {
      "displayName": "showSearchInput",
      "type": "boolean",
      "description": "是否显示搜索框",
      "defaultValue": "true",
      "tip": ""
    },
    {
      "displayName": "showFilter",
      "type": "boolean",
      "description": "是否显示筛选功能按钮",
      "defaultValue": "true",
      "tip": ""
    },
    {
      "displayName": "showRefresh",
      "type": "boolean",
      "description": "是否显示刷新按钮",
      "defaultValue": "true",
      "tip": ""
    },
    {
      "displayName": "showTransfer",
      "type": "boolean",
      "description": "是否显示列头控制穿梭框",
      "defaultValue": "false",
      "tip": ""
    },
    {
      "displayName": "widgets",
      "type": "(string | {id: string, widget: Component | () => VNode})[]",
      "description": "自定义表头组件，设置该参数时，showSearchInput、showFilter、showRefresh、showTransfer参数将失效,该参数数组提供了search、filter、refresh、transfer字段用于控制以上四个参数的显示与顺序，此外还可以自定义组件，参数的id字段用于标识组件，widget字段用于渲染组件，当widget字段为null时，会自动渲染名为id的插槽",
      "defaultValue": "",
      "tip": ""
    },
    {
      "displayName": "advancedFilterConfig",
      "type": "object",
      "description": "高级筛选配置",
      "defaultValue": "",
      "tip": "advancedFilterConfig"
    },
    {
      "displayName": "paginationConfig",
      "type": "object",
      "description": "分页配置",
      "defaultValue": "",
      "tip": "PaginationConfigType"
    },
    {
      "displayName": "searchConfig",
      "type": "object",
      "description": "搜索配置",
      "defaultValue": "",
      "tip": "searchConfigType"
    },
    {
      "displayName": "seq-config",
      "type": "object",
      "description": "搜索配置",
      "defaultValue": "",
      "tip": "seqConfigType"
    },
    {
      "displayName": "simple",
      "type": "boolean",
      "description": "是否开启简易模式，该模式下只保留表格主体与搜索功能，其他功能均隐藏，搜索功能也可以通过show-search-input参数控制显示/隐藏",
      "defaultValue": "false",
      "tip": "",
      "version": "0.0.67"
    },
    {
      "displayName": "default-transfer-data",
      "type": "object",
      "description": "默认的列头控制穿梭框数据，数据格式为Array<{label: string, key: string, disabled: boolean, visible: boolean}>",
      "defaultValue": "",
      "tip": "(() = > TableHeaderControl[] | Promise<TableHeaderControl[]>) | TableHeaderControl[]",
      "version": "0.0.72"
    },
    {
      "displayName": "on-transfer-show",
      "type": "Function",
      "description": "列头控制穿梭框显示时回调",
      "defaultValue": "",
      "tip": "() => void",
      "version": "0.0.72"
    },
    {
      "displayName": "on-transfer-hide",
      "type": "Function",
      "description": "列头控制穿梭框隐藏时回调",
      "defaultValue": "",
      "tip": "(transferData: TableHeaderControl[]) => void",
      "version": "0.0.72"
    },
    {
      "displayName": "on-advanced-filter-show",
      "type": "Function",
      "description": "高级筛选显示时回调",
      "defaultValue": "",
      "tip": "() => void",
      "version": "0.0.72"
    },
    {
      "displayName": "on-advanced-filter-hide",
      "type": "Function",
      "description": "高级筛选关闭时的回调",
      "defaultValue": "",
      "tip": "(filterData: FilterDataType) => void",
      "version": "0.0.72"
    },
    {
      "displayName": "use-ant-style",
      "type": "boolean",
      "description": "是否使用ant-design样式",
      "defaultValue": "false",
      "tip": "",
      "version": "0.0.74"
    },
    {
      "displayName": "adaptive",
      "type": "boolean",
      "description": "是否自适应内容高度",
      "defaultValue": "false",
      "version": "0.0.77"
    }
  ],
  "subProps": {
    "columnConfigType": [
      {
        "displayName": "title",
        "description": "列头显示文字",
        "type": "string"
      },
      {
        "displayName": "field",
        "description": "列字段名",
        "type": "string"
      },
      {
        "displayName": "width",
        "description": "列宽度",
        "type": "number | string"
      },
      {
        "displayName": "minWidth",
