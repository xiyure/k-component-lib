      },
      {
        "displayName": "sortable",
        "description": "是否可排序",
        "type": "boolean"
      },
      {
        "displayName": "showIcon",
        "description": "是否显示图标",
        "type": "boolean"
      },
      {
        "displayName": "visible",
        "description": "是否显示该列",
