    {
      "displayName": "reset",
      "description": "重置时触发",
      "type": "Function",
      "tip": "() => void"
    },
    {
      "displayName": "change",
      "description": "重置时触发",
      "type": "Function",
      "tip": "(vale: FormValidateCallback) => void"
