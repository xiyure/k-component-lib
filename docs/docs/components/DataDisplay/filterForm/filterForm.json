{
  "name": "FilterForm",
  "props": [
    {
      "displayName": "items",
      "type": "FilterFormItem[]",
      "description": "展示的数据项",
      "defaultValue": "[]",
      "tip": ""
    },
    {
      "displayName": "size",
      "type": "enum",
      "description": "尺寸",
      "defaultValue": "",
      "tip": "'base' | 'sm' | undefined"
    },
    {
      "displayName": "collapse",
      "type": "boolean",
      "description": "是否折叠",
      "defaultValue": "true"
    },
    {
      "displayName": "reserve",
      "type": "boolean",
      "description": "是否保留已选条件",
      "defaultValue": "false"
    },
    {
      "displayName": "visible",
