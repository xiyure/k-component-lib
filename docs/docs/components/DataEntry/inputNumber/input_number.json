{
  "name": "InputNumber",
  "props": [
    {
      "displayName": "model-value/v-model",
      "type": "number",
      "description": "绑定值",
      "defaultValue": "",
      "tip": ""
    },
    {
      "displayName": "min",
      "type": "number",
      "description": "最小值",
      "defaultValue": "-Infinity",
      "tip": ""
    },
    {
      "displayName": "max",
      "type": "number",
      "description": "最大值",
      "defaultValue": "Infinity"
    },
    {
      "displayName": "step",
      "type": "number",
      "description": "计数步长",
      "defaultValue": "1"
    },
    {
      "displayName": "size",
      "type": "enum",
      "description": "输入框大小",
      "defaultValue": "base",
      "tip": "'base' | 'sm'"
    },
    {
      "displayName": "step-strictly",
      "type": "boolean",
      "description": "是否只能输入 step 的倍数",
      "defaultValue": "false"
    },
    {
      "displayName": "precision",
      "type": "number",
      "description": "数值精度",
      "defaultValue": ""
    },
    {
      "displayName": "disabled",
      "type": "boolean",
      "description": "是否禁用",
      "defaultValue": "false"
    },
    {
      "displayName": "readonly",
      "type": "boolean",
      "description": "是否只读",
      "defaultValue": "false"
    },
    {
      "displayName": "controls",
      "type": "boolean",
      "description": "是否使用控制按钮",
      "tip": ""
    },
    {
      "displayName": "controls-position",
      "type": "enum",
      "description": "控制按钮位置",
      "tip": "'' | 'right'"
    },
    {
      "displayName": "only-digits",
      "type": "boolean",
      "description": "是否只能输入数字，正负号、小数点、科学计数法等都不允许输入",
      "defaultValue": "false",
      "tip": "",
      "version": "1.0.23"
    }
  ],
  "events": [
    {
      "displayName": "blur",
      "description": "输入框失去焦点时触发",
      "type": "Function",
      "tip": "(event: FocusEvent) => void"
    },
    {
      "displayName": "focus",
      "description": "输入框获得焦点时触发",
      "type": "Function",
      "tip": "(event: FocusEvent) => void"
    },
    {
      "displayName": "change",
      "description": "仅当 modelValue 改变时，当输入框失去焦点或用户按Enter时触发",
      "type": "Function",
      "tip": "(value: string | number) => void"
    }
  ],
  "slots": [
    {
      "displayName": "decrease-icon",
      "description": "自定义输入框按钮减少图标",
      "parameters": ""
    },
    {
      "displayName": "increase-icon",
      "description": "自定义输入框按钮增加图标",
      "parameters": ""
    }
  ],
  "methods": [
    {
      "displayName": "blur",
      "description": "使输入框失去焦点",
      "type": "Function",
      "tip": "() => void"
    },
    {
      "displayName": "focus",
      "description": "使输入框获得焦点",
      "type": "Function",
      "tip": "() => void"
    }
  ]
}
