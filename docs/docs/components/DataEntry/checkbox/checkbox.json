      "defaultValue": ""
    },
    {
      "displayName": "true-value",
      "type": "string | number",
      "description": "选中时的值",
      "defaultValue": ""
    },
    {
      "displayName": "false-value",
      "type": "string | number",
      "description": "未选中时的值",
      "defaultValue": ""
    },
    {
      "displayName": "size",
      "type": "enum",
      "description": "checkbox尺寸",
      "defaultValue": "base",
      "tip": "'base' | 'sm'"
    },
    {
      "displayName": "disabled",
      "type": "boolean",
      "description": "是否禁用",
      "defaultValue": "false"
    },
    {
      "displayName": "border",
      "type": "boolean",
      "description": "是否显示边框",
      "defaultValue": "false"
    },
    {
      "displayName": "checked",
