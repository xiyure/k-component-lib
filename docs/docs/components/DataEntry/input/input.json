{
  "name": "Input",
  "props": [
    {
      "displayName": "model-value/v-model",
      "type": "string | number",
      "description": "绑定值",
      "defaultValue": "",
      "tip": ""
    },
    {
      "displayName": "type",
      "type": "string",
      "description": "类型",
      "defaultValue": "text",
      "tip": "'text' | 'textarea' | 'password' | 'button' | 'checkbox' | 'file' | 'number' | 'radio' | ..."
    },
    {
      "displayName": "placeholder",
      "type": "string",
      "description": "输入框占位文本",
      "defaultValue": ""
    },
    {
      "displayName": "clearable",
      "type": "boolean",
      "description": "是否显示清除按钮，type不为textarea时生效",
      "defaultValue": "false"
    },
    {
      "displayName": "size",
      "type": "enum",
      "description": "输入框大小",
      "defaultValue": "base",
      "tip": "'base' | 'sm'"
    },
    {
      "displayName": "prefix-icon",
      "type": "string | Component",
      "description": "自定义前缀图标",
      "defaultValue": ""
    },
    {
      "displayName": "suffix-icon",
      "type": "string | Component",
      "description": "自定义后缀图标",
      "defaultValue": ""
    },
    {
      "displayName": "disabled",
      "type": "boolean",
      "description": "是否禁用",
      "defaultValue": "false"
    },
    {
      "displayName": "show-password",
      "type": "boolean",
      "description": "是否显示密码，type为password时生效",
      "defaultValue": "false"
    },
    {
      "displayName": "formatter",
      "type": "Function",
      "description": "格式化输入值, type为text时生效",
      "tip": "(value: string) => string"
    },
    {
      "displayName": "selectable",
      "type": "boolean",
      "description": "是否开启下拉选择功能",
      "tip": "false",
      "version":"1.1.19"
    },
    {
      "displayName": "options",
      "type": "Array<string | number>",
      "description": "下拉选择的选项列表",
      "tip": "() => []",
      "version":"1.1.19"
    },
    {
      "displayName": "teleported",
      "type": "boolean",
      "description": "是否将 popover 的下拉列表插入至 body 元素",
      "tip": "true",
      "version":"1.1.19"
    },
    {
      "displayName": "popper-class",
      "type": "string",
      "description": "为 popper 添加类名",
      "tip": "",
      "version":"1.1.19"
    },
    {
      "displayName": "popper-style",
      "type": "object",
      "description": "为 popper 自定义样式",
      "tip": "CSSProperties",
      "version":"1.1.19"
    },
    {
      "displayName": "filterable",
      "type": "boolean",
      "description": "用于selectable为true时，是否开启输入过程中过滤下拉选项功能",
      "tip": "false",
      "version":"1.1.20"
    }
  ],
  "events": [
    {
      "displayName": "blur",
      "description": "输入框失去焦点时触发",
      "type": "Function",
      "tip": "(event: FocusEvent) => void"
    },
    {
      "displayName": "focus",
      "description": "输入框获得焦点时触发",
      "type": "Function",
      "tip": "(event: FocusEvent) => void"
    },
    {
      "displayName": "change",
      "description": "仅当 modelValue 改变时，当输入框失去焦点或用户按Enter时触发",
      "type": "Function",
      "tip": "(value: string | number) => void"
    },
    {
      "displayName": "input",
      "description": "输入框内容变化时触发",
      "type": "Function",
      "tip": "(value: string | number) => void"
    },
    {
      "displayName": "clear",
      "description": "在点击由 clearable 属性生成的清空按钮时触发",
      "type": "Function",
      "tip": "() => void"
    },
    {
      "displayName": "popper-show",
      "description": "下拉列表显示时触发",
      "type": "Function",
      "tip": "() => void",
      "version":"1.1.19"
    },
    {
      "displayName": "popper-hide",
      "description": "下拉列表隐藏时触发",
      "type": "Function",
      "tip": "() => void",
      "version":"1.1.19"
    }
  ],
  "slots": [
    {
      "displayName": "default",
      "description": "下拉框无数据时显示的内容",
      "parameters": "",
      "version":"1.1.19"
    },
    {
      "displayName": "prefix",
      "description": "输入框头部内容，只对非type='textarea' 有效",
      "parameters": ""
    },
    {
      "displayName": "suffix",
      "description": "输入框尾部内容，只对非type='textarea' 有效",
      "parameters": ""
    },
    {
      "displayName": "prepend",
      "description": "自定义前置内容, 只对非type='textarea' 有效",
      "parameters": ""
    },
    {
      "displayName": "append",
      "description": "自定义后置内容, 只对非type='textarea' 有效",
      "parameters": ""
    }
  ],
  "methods": [
    {
      "displayName": "blur",
      "description": "使输入框失去焦点",
      "type": "Function",
      "tip": "() => void"
    },
    {
      "displayName": "focus",
      "description": "使输入框获得焦点",
      "type": "Function",
      "tip": "() => void"
    },
    {
      "displayName": "clear",
      "description": "清空输入框内容",
      "type": "Function",
      "tip": "() => void"
    },
    {
      "displayName": "select",
      "description": "选中输入框内容",
      "type": "Function",
      "tip": "() => void"
    },
    {
      "displayName": "resizeTextarea",
      "description": "改变textarea的大小",
      "type": "Function",
      "tip": "() => void"
    }
  ]
}
