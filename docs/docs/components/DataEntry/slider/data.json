{
  "name": "",
  "props": [
    {
      "displayName": "model-value / v-model",
      "type": "number / object",
      "description": "选中项绑定值",
      "defaultValue": 0
    },
    {
      "displayName": "min",
      "type": "number",
      "description": "最小值",
      "defaultValue": 0
    },
    {
      "displayName": "max",
      "type": "number",
      "description": "最大值",
      "defaultValue": 100
    },
    {
      "displayName": "disabled",
      "type": "boolean",
      "description": "是否禁用",
      "defaultValue": false
    },
    {
      "displayName": "step",
      "type": "number",
      "description": "步长",
      "defaultValue": 1
    },
    {
      "displayName": "show-input",
      "type": "boolean",
      "description": "是否显示输入框，仅在非范围选择时有效",
      "defaultValue": false
    },
    {
      "displayName": "show-input-controls",
      "type": "boolean",
      "description": "在显示输入框的情况下，是否显示输入框的控制按钮",
      "defaultValue": true
    },
    {
      "displayName": "size",
      "type": "enum",
      "description": "slider 包装器的大小，垂直模式下该属性不可用",
      "defaultValue": "default"
    },
    {
      "displayName": "input-size",
      "type": "enum",
      "description": "输入框的大小，如果设置了 size 属性，默认值自动取 size",
      "defaultValue": "default"
    },
    {
      "displayName": "show-stops",
      "type": "boolean",
      "description": "是否显示间断点",
      "defaultValue": false
    },
    {
      "displayName": "show-tooltip",
      "type": "boolean",
      "description": "是否显示提示信息",
      "defaultValue": true
    },
    {
      "displayName": "format-tooltip",
      "type": "Function",
      "description": "格式化提示信息",
      "defaultValue": ""
    },
    {
      "displayName": "range",
      "type": "boolean",
      "description": "是否开启选择范围",
      "defaultValue": false
    },
    {
      "displayName": "vertical",
      "type": "boolean",
      "description": "垂直模式",
      "defaultValue": false
    },
    {
      "displayName": "height",
      "type": "string",
      "description": "滑块高度，垂直模式必填",
      "defaultValue": ""
    },
    {
      "displayName": "label",
      "type": "string",
      "description": "屏幕阅读器标签",
      "defaultValue": ""
    },
    {
      "displayName": "range-start-label",
      "type": "string",
      "description": "当 range 为 true 时，屏幕阅读器标签开始的标记",
      "defaultValue": ""
    },
    {
      "displayName": "range-end-label",
      "type": "string",
      "description": "当 range 为 true 时，屏幕阅读器标签结尾的标记",
      "defaultValue": ""
    },
    {
      "displayName": "format-value-text",
      "type": "Function",
      "description": "显示屏幕阅读器的 aria-valuenow 属性的格式",
      "defaultValue": ""
    },
    {
      "displayName": "debounce",
      "type": "number",
      "description": "输入时的去抖延迟，毫秒，仅在 show-input 等于 true 时有效",
      "defaultValue": 300
    },
    {
      "displayName": "tooltip-class",
      "type": "string",
      "description": "tooltip 的自定义类名",
      "defaultValue": ""
    },
    {
      "displayName": "placement",
      "type": "enum",
      "description": "Tooltip 出现的位置",
      "defaultValue": "top"
    },
    {
      "displayName": "marks",
      "type": "object",
      "description": "标记， key 的类型必须为 number 且取值在闭区间 [min, max] 内，每个标记可以单独设置样式",
      "defaultValue": ""
    },
    {
      "displayName": "validate-event",
      "type": "boolean",
      "description": "输入时是否触发表单的校验",
      "defaultValue": true
    }
  ],
  "events": [
    {
      "displayName": "change",
      "type": "Function",
      "description": "值改变时触发（使用鼠标拖曳时，只在松开鼠标后触发）",
      "defaultValue": "",
      "tip": "(value: Arrayable<number>) => boolean"
    },
    {
      "displayName": "input",
      "type": "Function",
      "description": "数据改变时触发（使用鼠标拖曳时，活动过程实时触发）",
      "defaultValue": "",
      "tip": "(value: Arrayable<number>) => boolean"
    }
  ],
  "slots": [],
  "methods": []
}
